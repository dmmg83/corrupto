<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ttipocategoria" table="ttipocategoria">
    <id name="pkidtipocategoria" type="integer" column="pkidtipocategoria">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombretipocategoria" type="string" column="Nombretipocategoria" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="activotipocategoria" type="boolean" column="Activotipocategoria" nullable="false"/>
  </entity>
</doctrine-mapping>
